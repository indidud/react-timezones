{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-react-jsx"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "babel-plugin-transform-react-remove-prop-types", {
            "mode": "unsafe-wrap"
          }
        ]
      ]
    }
  }
}
